    "    if list_valeur:\n",
    "        return sum(list_valeur) / len(list_valeur)\n",
    "    else:\n",
    "        return 0\n",
    "# Test\n",
    "liste = [10, 5, 6, 2, 4]\n",
    "print(\"Moyenne exacte :\", average(liste))"
